{
  "manifest_version": 3,
  "name": "chatGPT repairman",
  "description": "repair chatGPT when connection is lost",
  "version": "1.0",
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "https://chat.openai.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}